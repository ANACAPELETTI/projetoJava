                        <Label fx:id="texto" alignment="CENTER" prefHeight="491.0" prefWidth="388.0" styleClass="lugarTextoEImagemOriginal" stylesheets="@styles.css">
                           <padding>
                              <Insets left="5.0" />
                           </padding></Label>
                     </content>
                     <VBox.margin>
                        <Insets left="5.0" />
                     </VBox.margin>
                  </ScrollPane>
                  <HBox prefHeight="50.0" prefWidth="394.0" spacing="20.0">
                     <children>
                        <Button fx:id="btReiniciar" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" styleClass="botoes" stylesheets="@styles.css" text="Reiniciar">
                           <HBox.margin>
                              <Insets left="10.0" />
